{
  "name": "@dobromyslov/abstract-request",
  "version": "0.9.12",
  "description": "Lightweight abstract request library for API clients implementation",
  "license": "MIT",
  "repository": "dobromyslov/abstract-request",
  "author": "Viacheslav Dobromyslov <viacheslav@dobromyslov.ru>",
  "keywords": [
    "fetch",
    "node-fetch",
    "request",
    "api client",
    "http"
  ],
  "files": [
    "dist"
  ],
  "main": "dist",
  "types": "dist",
  "sideEffects": false,
  "scripts": {
    "test": "xo && tsc --noEmit && nyc jasmine-ts",
    "release": "np",
    "build": "del-cli dist && tsc",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.4",
    "abort-controller": "^3.0.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jasmine": "^3.5.4",
    "@types/node": "^10.17.15",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "del-cli": "^3.0.0",
    "eslint-config-xo-typescript": "^0.26.0",
    "jasmine": "^3.5.0",
    "jasmine-ts": "^0.3.0",
    "np": "^6.1.0",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "xo": "^0.26.1"
  },
  "xo": {
    "extends": [
      "xo-typescript"
    ],
    "extensions": [
      "ts"
    ],
    "ignores": [
      "spec/"
    ],
    "space": 4,
    "rules": {
      "@typescript-eslint/indent": "off"
    }
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/**"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcovonly",
      "text",
      "text-summary"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
